function countOccurrences(arr) {
    let a = {}
    for(let i= 0; i < arr.length; i++){
        if(a[arr[i]]){
            a[arr[i]]++
        }
        else{
            a[arr[i]]=1
        }
    }
    return a
}
console.log(countOccurrences(["apple", "banana", "apple", "orange", "banana", "apple"])); 
// Output: { apple: 3, banana: 2, orange: 1 }
